		http://www.opengis.net/om/1.0
		http://www.opengis.net/omx/1.0
		namespaces </description>
	<imports>
		<schema
			prefix="om"
			namespace="http://www.opengis.net/om/1.0"
			schemaLocation="http://schemas.opengis.net/om/1.0.0/om.xsd"/>
		<schema
			prefix="omx"
			namespace="http://www.opengis.net/omx/1.0"
			schemaLocation="http://schemas.opengis.net/omx/1.0.0/om_extended.xsd"/>
	</imports>
	<entry>
		<umlClass>OM_CategoryObservation</umlClass>
		<gmlObjectElement>omx:CategoryObservation</gmlObjectElement>
		<gmlType>om:ObservationType</gmlType>
		<gmlPropertyType>om:ObservationPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>OM_CountObservation</umlClass>
		<gmlObjectElement>om:CountObservation</gmlObjectElement>
		<gmlType>om:ObservationType</gmlType>
		<gmlPropertyType>om:ObservationPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>OM_TruthObservation</umlClass>
		<gmlObjectElement>omx:TruthObservation</gmlObjectElement>
		<gmlType>om:ObservationType</gmlType>
		<gmlPropertyType>om:ObservationPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>OM_GeometryObservation</umlClass>
		<gmlObjectElement>omx:GeometryObservation</gmlObjectElement>
		<gmlType>om:ObservationType</gmlType>
		<gmlPropertyType>om:ObservationPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>OM_TemporalObservation</umlClass>
		<gmlObjectElement>omx:TemporalObservation</gmlObjectElement>
		<gmlType>om:ObservationType</gmlType>
		<gmlPropertyType>om:ObservationPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>OM_ComplexObservation</umlClass>
		<gmlObjectElement>om:ComplexObservation</gmlObjectElement>
		<gmlType>om:ObservationType</gmlType>
		<gmlPropertyType>om:ObservationPropertyType</gmlPropertyType>
	</entry>
</mapping>
