<?xml version="1.0" encoding="UTF-8"?>
<mapping
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:gml="http://www.opengis.net/gml"
	xmlns="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0"
	xsi:schemaLocation="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0 https://fullmoon-framework.googlecode.com/svn/trunk/resources/dependency-util/package-class-map.xsd"
	id="ClassMap_ISO_19156_General_Feature_Instance_GML31.xml">
	<description>Provides a mapping of the classes in the Oand M 2.0 model, General Feature Instance package, to
		their implementation in OMXML in the 
		http://www.opengis.net/gml
	    namespaces </description>
	<imports>
		<schema
			prefix="gml"
			namespace="http://www.opengis.net/gml"
			schemaLocation="http://schemas.opengis.net/gml/3.1.1/base/gml.xsd"/>
	</imports>
	<entry>
		<umlClass>GFI_Feature</umlClass>
		<gmlObjectElement>gml:AbstractFeature</gmlObjectElement>
		<gmlType>gml:AbstractFeatureType</gmlType>
		<gmlPropertyType>gml:FeaturePropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>GF_PropertyType</umlClass>
		<gmlObjectElement xsi:nil="true" />
		<gmlType>xsd:anyType</gmlType>
		<gmlPropertyType xsi:nil="true" />
	</entry>
</mapping>
