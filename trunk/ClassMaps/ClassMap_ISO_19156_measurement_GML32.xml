<?xml version="1.0" encoding="UTF-8"?>
<mapping
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:om="http://www.opengis.net/om/2.0"
	xmlns="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0"
	xsi:schemaLocation="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0 https://fullmoon-framework.googlecode.com/svn/trunk/resources/dependency-util/package-class-map.xsd"
	id="ClassMap_ISO_19156_measurement_GML32.xml">
	<description>Provides a mapping of the classes in the Oand M 2.0 model, measurement package, to
		their implementation in OMXML in the 
		http://www.opengis.net/om/2.0
	    namespaces </description>
	<imports>
		<schema
			prefix="om"
			namespace="http://www.opengis.net/om/2.0"
			schemaLocation="http://schemas.opengis.net/om/2.0/observation.xsd"/>
	</imports>
	<entry>
		<umlClass>OM_Measurement</umlClass>
		<gmlObjectElement>om:OM_Observation</gmlObjectElement>
		<gmlType>om:OM_ObservationType</gmlType>
		<gmlPropertyType>om:OM_ObservationPropertyType</gmlPropertyType>
	</entry>
</mapping>
