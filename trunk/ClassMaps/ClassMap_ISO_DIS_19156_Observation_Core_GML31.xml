<?xml version="1.0" encoding="UTF-8"?>
<mapping
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:om="http://www.opengis.net/om/1.0"
	xmlns:swe="http://www.opengis.net/swe/1.0.1"
	xmlns:sml="http://www.opengis.net/sensorML/1.0.1"
	xmlns="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0"
	xsi:schemaLocation="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0 http://projects.arcs.org.au/svn/fullmoon/trunk/resources/dependency-util/package-class-map.xsd"
	id="ClassMap_ISO_DIS_19156_Observation_Core_GML31.xml">
	<description>Provides a mapping of the classes in the Oand M 2.0 model, Coverage Observation package, to
		their implementation in OMXML 1.0 in the 
		http://www.opengis.net/om/1.0
		http://www.opengis.net/swe/1.0.1
		http://www.opengis.net/sensorML/1.0.1
		namespaces </description>
	<imports>
		<schema
			prefix="om"
			namespace="http://www.opengis.net/om/1.0"
			schemaLocation="http://schemas.opengis.net/om/1.0.0/om.xsd"/>
		<schema
			prefix="swe"
			namespace="http://www.opengis.net/swe/1.0.1"
			schemaLocation="http://schemas.opengis.net/sweCommon/1.0.1/swe.xsd"/>
		<schema
			prefix="sml"
			namespace="http://www.opengis.net/sensorML/1.0.1"
			schemaLocation="http://schemas.opengis.net/sensorML/1.0.1/sensorML.xsd"/>
	</imports>
	<entry>
		<umlClass>OM_Observation</umlClass>
		<gmlObjectElement>om:Observation</gmlObjectElement>
		<gmlType>om:ObservationType</gmlType>
		<gmlPropertyType>om:ObservationPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>OM_Process</umlClass>
		<gmlObjectElement
			xsi:nil="true"/>
		<gmlType>xsd:anyType</gmlType>
		<gmlPropertyType>om:ProcessPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>ObservationContext</umlClass>
		<gmlObjectElement
			xsi:nil="true"/>
		<gmlType
			xsi:nil="true"/>
		<gmlPropertyType
			xsi:nil="true"/>
	</entry>
	<entry>
		<umlClass>NamedValue</umlClass>
		<gmlObjectElement>swe:AnyData</gmlObjectElement>
		<gmlType
			xsi:nil="true"/>
		<gmlPropertyType>swe:AnyDataPropertyType</gmlPropertyType>
	</entry>
</mapping>
