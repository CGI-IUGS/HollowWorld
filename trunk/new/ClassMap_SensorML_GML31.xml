<?xml version="1.0" encoding="UTF-8"?>
<mapping
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:sml="http://www.opengis.net/sensorML/1.0.1"
	xmlns="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0"
	xsi:schemaLocation="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0 https://www.seegrid.csiro.au/subversion/FullMoon/trunk/resources/type-mapping-util/package-class-map.xsd"
	id="ClassMap_SensorML_GML31.xml">
	<description>Provides a mapping of the classes in the SensorML model to their implementation in the http://www.opengis.net/sensorML/1.0.1 XML Schema
	GML 3.1 binding</description>
	<imports>
		<schema prefix="sml" namespace="http://www.opengis.net/sensorML/1.0.1" schemaLocation="http://schemas.opengis.net/sensorML/1.0.1/sensorML.xsd" />
	</imports>
	<entry>
		<umlClass>Process</umlClass>
		<gmlObjectElement>sml:_Process</gmlObjectElement>
		<gmlType>sml:AbstractProcessType</gmlType>
		<gmlPropertyType xsi:nil="true" />
	</entry>
	<entry>
		<umlClass>ProcessModel</umlClass>
		<gmlObjectElement>sml:ProcessModel</gmlObjectElement>
		<gmlType>sml:ProcessModelType</gmlType>
		<gmlPropertyType xsi:nil="true" />
	</entry>
	<entry>
		<umlClass>ProcessSystem</umlClass>
		<gmlObjectElement>sml:ProcessChain</gmlObjectElement>
		<gmlType>sml:ProcessChainType</gmlType>
		<gmlPropertyType xsi:nil="true" />
	</entry>
</mapping>
