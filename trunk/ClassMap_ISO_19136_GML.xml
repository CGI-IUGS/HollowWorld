<?xml version="1.0" encoding="UTF-8"?>
<mapping 
xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
xmlns:gml="http://www.opengis.net/gml/3.2"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="urn:tbd:xmlns:TBD:FullMoon:type-mapping:1.0"
xsi:schemaLocation="urn:tbd:xmlns:TBD:FullMoon:type-mapping:1.0 https://www.seegrid.csiro.au/subversion/FullMoon/trunk/resources/type-mapping-util/type-mapping.xsd"
id="ClassMap_ISO_19136_GML.xml">
	<description>Provides a mapping of the classes listed in Table D.2 found in Annex D of the Internation Standard ISO 19136 (GML Version
		3.2), 2007 but omitting those that correspond directly with classes in other ISO 19100 series standards</description>
	<package id="ClassMap_primitive_Types">
		<description>Provides a mapping of primitive types built into most UML tools to their XML implementation (mostly in GML)
		Note that ISO 19103 types are preferred. </description>
		<entry>
			<umlClass>boolean</umlClass>
			<gmlObjectElement xsi:nil="true"/>
			<gmlType xsi:nil="true"/>
			<gmlPropertyType>xsd:boolean</gmlPropertyType>
		</entry>
		<entry>
			<umlClass>byte</umlClass>
			<gmlObjectElement xsi:nil="true"/>
			<gmlType xsi:nil="true"/>
			<gmlPropertyType>xsd:byte</gmlPropertyType>
		</entry>
		<entry>
			<umlClass>char</umlClass>
			<gmlObjectElement xsi:nil="true"/>
			<gmlType xsi:nil="true"/>
			<gmlPropertyType>xsd:string</gmlPropertyType>
		</entry>
		<entry>
			<umlClass>double</umlClass>
			<gmlObjectElement xsi:nil="true"/>
			<gmlType xsi:nil="true"/>
			<gmlPropertyType>xsd:double</gmlPropertyType>
		</entry>
		<entry>
			<umlClass>float</umlClass>
			<gmlObjectElement xsi:nil="true"/>
			<gmlType xsi:nil="true"/>
			<gmlPropertyType>xsd:float</gmlPropertyType>
		</entry>
		<entry>
			<umlClass>int</umlClass>
			<gmlObjectElement xsi:nil="true"/>
			<gmlType xsi:nil="true"/>
			<gmlPropertyType>xsd:int</gmlPropertyType>
		</entry>
		<entry>
			<umlClass>long</umlClass>
			<gmlObjectElement xsi:nil="true"/>
			<gmlType xsi:nil="true"/>
			<gmlPropertyType>xsd:long</gmlPropertyType>
		</entry>
		<entry>
			<umlClass>short</umlClass>
			<gmlObjectElement xsi:nil="true"/>
			<gmlType xsi:nil="true"/>
			<gmlPropertyType>xsd:short</gmlPropertyType>
		</entry>
	</package>
	<package id="ClassMap_ISO_19136_Annex_D">
		<description>Classes not from other ISO 19100 standards</description>
		<entry>
			<umlClass>AnyDefinition</umlClass>
			<gmlObjectElement>gml:Definition</gmlObjectElement>
			<gmlType>gml:DefinitionType</gmlType>
			<gmlPropertyType>gml:DictionaryEntryType</gmlPropertyType>
		</entry>
		<entry>
			<umlClass>AnyDictionary</umlClass>
			<gmlObjectElement>gml:Dictionary</gmlObjectElement>
			<gmlType>gml:DictionaryType</gmlType>
			<gmlPropertyType xsi:nil="true" anonymous="true"/>
		</entry>
		<entry>
			<umlClass>AnyFeature</umlClass>
			<gmlObjectElement>gml:AbstractFeature</gmlObjectElement>
			<gmlType>gml:AbstractFeatureType</gmlType>
			<gmlPropertyType>gml:FeaturePropertyType</gmlPropertyType>
		</entry>

		<entry>
			<umlClass xsi:nil="true"/>
			<gmlObjectElement>gml:LineString</gmlObjectElement>
			<gmlType>gml:LineStringType</gmlType>
			<gmlPropertyType xsi:nil="true"/>
		</entry>
		<entry>
			<umlClass xsi:nil="true"/>
			<gmlObjectElement>gml:Polygon</gmlObjectElement>
			<gmlType>gml:PolygonType</gmlType>
			<gmlPropertyType xsi:nil="true"/>
		</entry>
		<entry>
			<umlClass xsi:nil="true"/>
			<gmlObjectElement>gml:LinearRing</gmlObjectElement>
			<gmlType>gml:LinearRingType</gmlType>
			<gmlPropertyType xsi:nil="true"/>
		</entry>

		<entry>
			<umlClass xsi:nil="true"/>
			<gmlObjectElement>gml:ArcByCenterPoint</gmlObjectElement>
			<gmlType>gml:ArcByCenterPointType</gmlType>
			<gmlPropertyType xsi:nil="true"/>
		</entry>

		<entry>
			<umlClass xsi:nil="true"/>
			<gmlObjectElement>gml:CircleByCenterPoint</gmlObjectElement>
			<gmlType>gml:CircleByCenterPointType</gmlType>
			<gmlPropertyType xsi:nil="true"/>
		</entry>

		<entry>
			<umlClass xsi:nil="true"/>
			<gmlObjectElement>gml:Rectangle</gmlObjectElement>
			<gmlType>gml:RectangleType</gmlType>
			<gmlPropertyType xsi:nil="true"/>
		</entry>

		<entry>
			<umlClass xsi:nil="true"/>
			<gmlObjectElement>gml:TopoPoint</gmlObjectElement>
			<gmlType>gml:TopoPointType</gmlType>
			<gmlPropertyType>gml:TopoPointPropertyType</gmlPropertyType>
		</entry>
		<entry>
			<umlClass xsi:nil="true"/>
			<gmlObjectElement>gml:TopoCurve</gmlObjectElement>
			<gmlType>gml:TopoCurveType</gmlType>
			<gmlPropertyType>gml:TopoCurvePropertyType</gmlPropertyType>
		</entry>
		<entry>
			<umlClass xsi:nil="true"/>
			<gmlObjectElement>gml:TopoSurface</gmlObjectElement>
			<gmlType>gml:TopoSurfaceType</gmlType>
			<gmlPropertyType>gml:TopoSurfacePropertyType</gmlPropertyType>
		</entry>
		<entry>
			<umlClass xsi:nil="true"/>
			<gmlObjectElement>gml:TopoVolume</gmlObjectElement>
			<gmlType>gml:TopoVolumeType</gmlType>
			<gmlPropertyType>gml:TopoVolumePropertyType</gmlPropertyType>
		</entry>
	</package>
</mapping>
